{
  "name": "react-query-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:mock\" \"pnpm run dev:app\"",
    "dev:app": "next dev",
    "dev:mock": "json-server --watch src/testing/mocks/db.json --port 3001 --routes src/testing/mocks/routes.json",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.29.25",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "axios-case-converter": "^1.1.0",
    "dayjs": "^1.11.9",
    "eslint-config-next": "13.4.10",
    "flowbite": "^1.7.0",
    "next": "13.4.10",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-hook-form": "^7.45.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@hookform/devtools": "^4.3.1",
    "@mswjs/data": "^0.13.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tanstack/eslint-plugin-query": "^4.34.1",
    "@types/jest": "^29.5.3",
    "@types/json-server": "^0.14.4",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "jest": "^29.6.1",
    "json-server": "^0.17.3",
    "msw": "^1.2.3",
    "msw-devtools": "^1.0.2",
    "prettier": "^3.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
